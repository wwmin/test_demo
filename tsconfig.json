{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "removeComments": true,
    "noImplicitAny": false,
    "sourceMap": false,
    "allowJs": true,
    "watch": true
  },
  "include": [
    "./src/**/*",
    "./service/*",
    "./**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}